/*
	Photo album trigger
*/


/*
	Photo album button
*/

.btnAlbumPhoto {

	background:transparent url(../../Resources/Images/UI/Navigation/Icones/Photo_16X16.png) no-repeat scroll 0 0;
	display:block;
	height:16px;
	margin-bottom:30px;
	overflow:hidden;
	text-indent:-999em;
	width:16px;
	cursor:pointer;
}


/*
	Overlaying window
*/

.simple_overlay {
	
	/* must be initially hidden */
	display:none;
	
	/* place overlay on top of other elements */
	z-index:10000;
	
	/* styling */

		
	/* growing background image */
	/*background-image:url(http://static.flowplayer.org/tools/img/overlay/white.png);*/
	background-image:url(../../Resources/Images/UI/Navigation/Overlay/white.png);
		
	width:640px;
	padding:35px;
	
	/****test conception***/
	 border-style:dotted;
	/**********************/
}

/* 
close button positioned on upper right corner  of Overlaying window
*/

.simple_overlay .close {

	background-image:url(../../Resources/Images/UI/Navigation/Overlay/close.png);
	position:absolute;
	right: 1px;
	top: 1px;	
	cursor:pointer;
	height:35px;
	width:35px;
}

.simple_overlay .contentWrap
{
		/*dimensions*/
		
		height:75%;
		width:100%;
	
		/****test conception**/
		border:3px solid #3F9CFF;
		/*********************/
}

.simple_overlay .contentWrap .title {
	
	/* some "skinning" */
	
	color: #5b6f7b;
	font-family: Arial;
	font-size: 16pt;
	font-style: normal;
	font-weight: bold;
	
	text-align: center;
	margin-bottom: 0;
	margin-top: 10px;
	margin-right: 0;
	margin-left: 0;
	
	mc-hyphenate: never;		
}

.simple_overlay .contentWrap .subtitle {
	
	/* some "skinning" */
	
	color: #5b6f7b;
	font-family: Arial;
	font-size: 12pt;
	font-style: normal;
	font-weight: bold;
		
	text-align: center;
	margin-bottom: 10px;
	margin-top: 0;
	margin-right: 0;
	margin-left: 0;
	
	mc-hyphenate: never;			
}
	
/*
	Primary photo wraper in photo album
*/
.simple_overlay .contentWrap .image_wrap {
		
		/* dimensions */
		
		width:320px;
		height:240px;
		
		/* some "skinning" */
		
		text-align:center;
		margin-bottom: 10px;
		margin-top: 0;
		margin-right: 150px;
		margin-left: 150px;		
		
		/****test conception***/
		border-style:dashed;
		/**********************/
}

/*	
	Primary photo in photo album	
*/

.image_wrap img{

		/* dimensions */
		width:100%;
		height:100%;
	}	
	
/*
	root element for the scrollable.
	when scrolling occurs this element stays still.
*/

.simple_overlay .contentWrap .scrollable {

	/* required settings */
	position:relative;
	overflow:hidden;
	width:620px;
	height:100px;
	
	margin-bottom: 0;
	margin-top: 0;
	margin-right: 10px;
	margin-left: 10px;	
	

	/* custom decorations */
	border:1px solid #ccc;
	background:url(../../Resources/Images/UI/Couleurs/Degrades/h300.png) repeat-x;	
}

/*
	root element for scrollable items. Must be absolutely positioned
	and it should have a extremely large width to accomodate scrollable items.
	it's enough that you set the width and height for the root element and
	not for this element.
*/

.scrollable .items {
	/* this cannot be too large */
	/*width:20000em;*/
	width:100%;
	position:absolute;
	clear:both;
	
	/* centered */
	text-align:center;
	
	
	/****test conception***/
	border-style:solid;
	/**********************/
}

.scrollable .items div {
	float:left;
	width:100%;
	height:100%;
	
	/****test conception***/
	border-style:dotted;
	/**********************/
}

/* single scrollable item */
.scrollable img {
	float:left;
	margin:5px 10px 5px 10px;
	background-color:#fff;
	padding:2px;
	border:1px solid #ccc;
	width:100px;
	height:75px;
	cursor:pointer;
}

/* active item */
.scrollable .active {
	border:2px solid #000;
	position:relative;
	cursor:default;
}

/* this makes it possible to add next button beside scrollable */
.scrollable {
	float:left;	
}

/* prev, next, prevPage and nextPage buttons */
a.browse {
	background:url(../../Resources/Images/UI/Navigation/Scrollable/Fleches/hori_large.png) no-repeat;
	display:block;
	width:30px;
	height:30px;
	float:left;
	margin:40px 10px;
	cursor:pointer;
	font-size:1px;
}

/* right */
a.right 		{ background-position: 0 -30px; clear:right; margin-right: 0px;}
a.right:hover 	{ background-position:-30px -30px; }
a.right:active 	{ background-position:-60px -30px; } 


/* left */
a.left			{ margin-left: 0px; } 
a.left:hover  	{ background-position:-30px 0; }
a.left:active  	{ background-position:-60px 0; }

